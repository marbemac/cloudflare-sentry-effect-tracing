{
  "name": "cloudflare-sentry-effect-tracing",
  "private": true,
  "version": "0.0.0",
  "packageManager": "yarn@4.9.2",
  "type": "module",
  "scripts": {
    "dev": "vite --port=3005",
    "build": " vite build",
    "build.analyze": "vite build --analyze",
    "deploy": "wrangler deploy --var SENTRY_RELEASE:$(sentry-cli releases propose-version)",
    "lint": "eslint .",
    "test": "vitest run",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@opentelemetry/instrumentation": "0.205.0",
    "@orpc/client": "1.8.6",
    "@orpc/openapi": "1.8.6",
    "@orpc/otel": "1.8.6",
    "@orpc/server": "1.8.6",
    "@sentry/cloudflare": "10.10.0",
    "@sentry/core": "10.10.0",
    "@sentry/react": "10.10.0",
    "effect": "3.17.13",
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.12.3",
    "@cloudflare/vitest-pool-workers": "0.8.70",
    "@datanaut/eslint-config": "0.1.1",
    "@datanaut/tsconfig": "0.1.3",
    "@effect/language-service": "0.38.1",
    "@sentry/cli": "2.53.0",
    "@sentry/vite-plugin": "4.3.0",
    "@tailwindcss/vite": "4.1.13",
    "@tanstack/react-router": "1.131.36",
    "@tanstack/router-plugin": "1.131.36",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/node": "24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "5.0.2",
    "eslint": "^9.35.0",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.12",
    "type-fest": "4.41.0",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.42.0",
    "vite": "7.1.4",
    "vite-bundle-analyzer": "1.2.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4",
    "wrangler": "4.34.0"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "trailingComma": "all",
    "proseWrap": "always",
    "singleQuote": true,
    "overrides": [
      {
        "files": "package*.json",
        "options": {
          "printWidth": 1000
        }
      }
    ]
  }
}
